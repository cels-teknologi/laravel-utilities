{
  "name": "cels-teknologi/laravel-utilities",
  "description": "Laravel utilities and helpers",
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "guzzlehttp/guzzle": "^7.0.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "mockery/mockery": "^1.4.4",
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "Cels\\Utilities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cels\\Utilities\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cels\\Utilities\\ServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Stephen",
      "email": "stephen@cels.co.id"
    }
  ]
}
