{
  "name": "cels-teknologi/laravel-utilities",
  "description": "Laravel utilities and helpers",
  "require": {
    "php": "^8.2|^8.3|^8.4",
    "guzzlehttp/guzzle": "^7.0.1",
    "illuminate/support": "^11.0|^12.0",
    "league/oauth2-client": "^2.7"
  },
  "require-dev": {
    "mockery/mockery": "^1.4.4",
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "Cels\\Utilities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cels\\Utilities\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cels\\Utilities\\ServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Stephen",
      "email": "stephen@cels.co.id"
    }
  ]
}
